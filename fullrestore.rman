RUN {
set DBID &1;
startup nomount;
restore spfile from '/home/oracle/backup/SPFILE_BACKUP';
startup force nomount;
restore controlfile from '/home/oracle/backup/CONTROLFILE_BACKUP';
alter database mount;
catalog start with '/home/oracle/backup';
restore database;
recover database until available redo;
alter database open resetlogs;
}
QUIT;